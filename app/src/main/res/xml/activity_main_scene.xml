<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="10000"
        motion:layoutDuringTransition="honorRequest"
        motion:motionInterpolator="easeOut"
        motion:pathMotionArc="startVertical">
        <KeyFrameSet />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@+id/imageView21"
            motion:layout_constraintStart_toEndOf="@+id/imageView21" />
        <Constraint
            android:id="@+id/imageView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@+id/imageView19"
            motion:layout_constraintEnd_toEndOf="@+id/imageView19" />
        <Constraint
            android:id="@+id/imageView29"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/textView3"
            motion:layout_constraintTop_toBottomOf="@+id/imageView28" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/adView"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            motion:transitionEasing="accelerate" />
        <Constraint
            android:id="@+id/imageView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:rotationX="21"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toBottomOf="parent"
            motion:layout_editor_absoluteX="222dp"
            motion:pathMotionArc="flip"
            motion:transitionEasing="decelerate" />
        <Constraint
            android:id="@+id/imageView3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="1.0" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/start_hoa" >
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="@+id/imageView26"
            android:layout_width="wrap_content"
            android:id="@+id/imageView12"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView26" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/end_hoa" >
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="118dp"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="parent"
            android:id="@+id/imageView12"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="0dp"
            android:layout_marginRight="277dp"
            motion:transitionEasing="standard"
            motion:pathMotionArc="startVertical" />

    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/start_hoa"
        motion:constraintSetEnd="@+id/end_hoa"
        motion:autoTransition="animateToEnd"
        motion:motionInterpolator="easeOut"
        motion:duration="7000"
        motion:layoutDuringTransition="honorRequest"
         />
    <ConstraintSet android:id="@+id/start_hoa2" >
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="216dp"
            android:id="@+id/imageView13"
            motion:layout_constraintTop_toBottomOf="@+id/imageView14"
            motion:layout_constraintStart_toEndOf="@+id/imageView26" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="225dp"
            android:id="@+id/imageView14"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="297dp"
            android:id="@+id/imageView15"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/imageView3" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="165dp"
            android:id="@+id/imageView16"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/imageView3" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/end_hoa2" >
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView3"
            android:layout_width="wrap_content"
            android:id="@+id/imageView13"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="225dp"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="parent"
            android:id="@+id/imageView14"
            motion:layout_constraintStart_toEndOf="@+id/imageView13" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView15"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView16"
            motion:layout_constraintStart_toEndOf="@+id/imageView15"
            android:layout_marginStart="32dp" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/start_hoa2"
        motion:constraintSetEnd="@+id/end_hoa2"
        motion:autoTransition="animateToEnd"
        motion:duration="9000" />
</MotionScene>