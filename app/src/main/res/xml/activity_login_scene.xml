<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="0dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="0dp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.797" />
        <Constraint
            android:id="@+id/login_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/imageView" />
        <Constraint
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView2"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            motion:layout_editor_absoluteY="633dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintEnd_toStartOf="@+id/login_button"
            android:id="@+id/imageView7"
            motion:visibilityMode="normal"
            android:visibility="invisible"
            motion:layout_constraintTop_toBottomOf="@+id/login_button"
            android:layout_marginTop="0dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="@+id/login_button"
            android:layout_width="wrap_content"
            android:id="@+id/imageView9"
            motion:visibilityMode="normal"
            android:visibility="invisible"
            motion:layout_constraintStart_toStartOf="@+id/login_button"
            motion:layout_editor_absoluteY="352dp"
            motion:layout_constraintTop_toBottomOf="@+id/login_button" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView8"
            motion:visibilityMode="normal"
            android:visibility="invisible"
            motion:layout_constraintBottom_toTopOf="@+id/login_button"
            motion:layout_constraintStart_toEndOf="@+id/login_button" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView10"
            motion:visibilityMode="normal"
            android:visibility="invisible"
            motion:layout_constraintBottom_toTopOf="@+id/login_button"
            motion:layout_constraintStart_toStartOf="@+id/login_button"
            android:layout_marginBottom="150dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            motion:layout_constraintTop_toBottomOf="@+id/textView"
            motion:layout_constraintEnd_toEndOf="@+id/textView"
            motion:layout_constraintStart_toStartOf="@+id/textView" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/progress_bar"
            motion:visibilityMode="normal"
            android:visibility="invisible"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="@+id/textView2" />

    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/imageView"
            android:layout_width="@dimen/_100sdp"
            android:layout_height="@dimen/_200sdp"
            android:layout_marginStart="@dimen/_32sdp"
            android:layout_marginTop="@dimen/_32sdp"
            android:layout_marginEnd="@dimen/_32sdp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:transitionEasing="decelerate" />
        <Constraint
            android:id="@+id/login_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="visible"
            motion:visibilityMode="ignore"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/textView"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="@dimen/_32sdp" />
        <Constraint
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/imageView2"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            motion:layout_editor_absoluteY="370dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintEnd_toStartOf="@+id/login_button"
            android:id="@+id/imageView7"
            android:visibility="visible"
            motion:visibilityMode="normal"/>
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView8"
            android:visibility="visible"
            motion:visibilityMode="normal"
            motion:layout_constraintStart_toEndOf="@+id/login_button"
            motion:layout_constraintBottom_toTopOf="@+id/login_button" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="187dp"
            android:id="@+id/imageView9"
            android:visibility="visible"
            motion:visibilityMode="normal"
            motion:layout_constraintTop_toBottomOf="@+id/login_button" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="@+id/login_button"
            android:layout_width="wrap_content"
            android:id="@+id/imageView10"
            android:visibility="visible"
            motion:visibilityMode="normal"
            motion:layout_constraintStart_toStartOf="@+id/login_button"
            motion:layout_constraintBottom_toBottomOf="@+id/login_button" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="@+id/textView"
            android:layout_width="wrap_content"
            android:id="@+id/textView2"
            motion:layout_constraintTop_toBottomOf="@+id/textView"
            motion:layout_constraintStart_toStartOf="@+id/textView" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/progress_bar"
            android:visibility="invisible"
            motion:visibilityMode="normal"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="@+id/textView2" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/start"
        motion:constraintSetEnd="@+id/end"
        motion:duration="3000">
        <OnClick />
    </Transition>
</MotionScene>